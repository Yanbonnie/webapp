/* pages/member/order/order_list/order_list.wxss */
.order-item{
    margin-bottom: 20rpx;
    background: #ffffff;
    border-top: 1rpx #e0e0e0 solid;
    border-bottom: 1rpx #e0e0e0 solid;
}
.flex-box{
    display: flex;
    margin: 0 30rpx;
    justify-content: space-between;
    padding: 30rpx 0;
}
.txt{
    margin: 0 30rpx;
    line-height: 50rpx;
    border-top: 1rpx #e0e0e0 solid;
    border-bottom: 1rpx #e0e0e0 solid;
    padding: 30rpx 0;
}
.txt view{
    color: #808080;
}
.flex-box .yuyue{
    border:1rpx #fe6603 solid;
    background: #fe6603;
    padding: 3rpx 5rpx;
    color: #ffffff;
    border-radius: 8rpx;
    width: 100rpx;
    text-align: center;
}
.flex-box .finish{
    border:1rpx #1AAD19 solid;
    background: #1AAD19;
    padding: 3rpx 5rpx;
    color: #ffffff;
    border-radius: 8rpx;
    width: 100rpx;
    text-align: center;
}
.no-message{
    text-align: center;
    font-size: 32rpx;
    padding-top: 50rpx;
}
