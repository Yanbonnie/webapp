<!--pages/member/order/success/success.wxml-->
<view class='success'>
    <view class='blank'></view>
    <image  src='/assets/images/success.png'></image>
    <view class='success_txt'>下单成功</view>
    <view class='order_txt'>订单号：{{id}}</view>
    <view class='btnBox'>
        <view bindtap='goHomeHandle'>返回首页</view>
        <view bindtap='goOrderHandle'>查看订单</view>
    </view>
    <view class='text_list'>
        <text>客服联系电话:020-45434564</text>
        <text>如有疑问，请联系客服</text>
    </view>
</view>
