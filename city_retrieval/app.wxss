.contain_box{
    width: 100%;
     height: calc(100% - 60rpx); 
    overflow: hidden;
    position: relative;
    top: 60rpx;
}
.blankNav{
    width: 100%;
    height: 60rpx;
    display: block;
}
/*nav*/

page {
    margin: 0;
    padding: 0;
}
.nav {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    background: #3385ff;
    height: 60rpx;
    line-height: 60rpx;
    width: 100%;
    z-index: 2;
}

.nav view {
    /* display: block; */
    padding: 0 20rpx;
    font-size: 28rpx;
    align-self: center;
    color: #fff;
}

.nav image {
    width: 30rpx;
    height: 30rpx;
    display: inline-block;
    position: relative;
    top: 6rpx;
}
.nav .nav_right image{
    width: 35rpx;
    height: 35rpx;
    top: 10rpx;
}

.clearfix {
  overflow: auto;
  _height: 1%;
}

.shengl{
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}


