.editor_first {
    margin-top: 0;
}

.editor_first:before {
    display: none
}

.select_picker_text:after {
    content: ' ';
    border-style: solid;
    border-color: #999 transparent;
    border-width: 4px 4px 0 4px;
    display: inline-block;
    width: 0;
    height: 0;
    margin-left: 5px;
    vertical-align: middle;
    margin-top: -3px;
}

.btn-upload {
    background: #f3f5f5;
    color: #939da3;
    text-align: center;
    padding: 50px 0 0;
    position: relative;
    width: 100%;
    height: 300rpx;
    overflow: hidden;
}

.icon-add {
    width: 80rpx;
    height: 80rpx;
    margin: 10px auto;
    display: block;
}

.pic_thumb {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    height: 100%;
    background: #f2f2f2;
}
.teamItem{
    display: flex;
    justify-content: flex-start;
    align-items: center;
}
.team-icon{
    width: 40rpx;
    height: 40rpx;
    margin-left: 10rpx;
}
.clauseBox{
    width: 80%;
    position: relative;
}
.clauseBox .agree{
    display:flex;
    justify-content: flex-start;
    align-items: center;
    padding: 20rpx;
}
.clauseBox .agree image{
    width: 40rpx;
    height: 40rpx;
    margin-right: 10rpx;

}
.clauseBox .btn{
    margin: 20rpx auto;
    width: 80%;
    height: 70rpx;
    line-height: 70rpx;
    text-align: center;
    border-radius: 8rpx;
    color: #4a4a4a;
    background: #fde763;
}
.giftFlex input{
    width: 500rpx;
}
.num{
    position: absolute;
    right: 0;
    top: 0;
}