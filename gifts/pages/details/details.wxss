.page {
    font-size: 30rpx;
}

.userlist {
    position: relative;
    background: #fff;
    overflow: hidden;
    padding: 0 30rpx 30rpx 30rpx;
    margin-bottom: 20rpx;
}

.status {
    position: absolute;
    top: 13px;
    left: -25px;
    color: #575232;
    background: #fde763;
    font-size: 28rpx;
    padding: 3px 30px;
    transform: rotate(-45deg);
}

.disable {
    background: #cbcbcb;
}

.userlist-pic {
    width: 100%;
}

.userlist-pic image {
    width: 100%;
    height: auto;
}

.userlist-name {
    font-size: 38rpx;
    color: #333;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    margin: 10px 0;
    display: flex;
    align-items: center;
}
.userlist-name image{
    width: 35rpx;
    height: 35rpx;
    margin-right:10rpx;
}

.depth {
    padding: 16rpx 0;
}

.gradient {
    position: relative;
    margin: 0 12rpx;
    text-align: center;
    color: #333;
    padding: 12rpx 20rpx;
    white-space: nowrap;
}

.gradient text {
    position: relative;
    z-index: 1;
}

.gradient image {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
}

.detail {
    padding: 30rpx 0 0;
}

.detail .ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.detail .extend_fields {}

.participant {
    background: #fff;
    padding: 0 0 30rpx 0;
    position: relative;
}
.participant .more{
    position: absolute;
    right: 20rpx;
    top: 20rpx;
    z-index: 2;
    background: #fff;
    border-radius: 50rpx;
    color: #000;
    padding: 8rpx 15rpx;
    box-shadow:2rpx 2rpx 2rpx #ccc;
    display: inline-block;
    font-size: 24rpx;
}
.participant .title {
    float: left;
    width: 130rpx;
    height: 57rpx;
    line-height: 57rpx;
    text-align: center;
    border-radius: 60rpx;
    background: #f3f5f5;
    display: inline-block;
    color: #939da3;
    margin: 8rpx;
    position: relative;
    top: 3px;
}

.participant ul li {
    width: 57rpx;
    float: left;
    margin: 8rpx;
    height: 57rpx;
    padding-top: 3px;
}

.participant ul li.winner {
    background-size: 100%;
}

.participant ul li.winner image {
    border: 2px solid #fde23d;
    box-sizing: border-box;
}

.participant ul li image {
    width: 57rpx;
    height: 57rpx;
    border-radius: 50%;
}

.more-detail,
.more-detail-fixed,
.more-partner {
    color: #fcc600;
    text-align: center;
    padding-left: 10rpx;
}

.more-detail-fixed {
    position: absolute;
    bottom: -1px;
    right: 0;
    background: #fff;
}

.more-partner {
    margin: 10px 10px 0;
}

.winners-list .avatar {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
    background: #f5f5fa;
}

.winners-item {
    padding: 10px 15px
}

.winners-item:before {
    left: 15px;
    right: 15px;
}

.item-l,
.item-r {
    text-align: center;
}

.item-m {
    color: #333;
}

.item-m .time {
    font-size: 28rpx;
    margin-top: 10rpx;
}

.item-l {
    width: 180rpx;
    margin-left: -10px;
}

.item-r {
    margin-left: 15px;
}

.item-r .btn_cl {
    font-size: inherit;
    padding: 20rpx 30rpx;
    line-height: 1;
    border-radius: 20px;
    color: #fff;
    margin-top: 10rpx;
}

.status-yellow .btn_cl {
    background: #fde23d;
}

.status-blue .btn_cl {
    background: #75d4ff;
}

.status-yellow .m-status {
    color: #fbc31d;
}

.status-blue .m-status {
    color: #8dc6e6;
}

.item-r .btn_cl::after {
    border: none;
}

.item-m .m-status {
    font-size: 30rpx;
    margin-bottom: 10px
}

.ft_box {
    padding: 20px 15px;
}
.share-footer{
    display: flex;
    
}
.share-footer button{
    flex-grow: 1;
}
.btn-commit {
    background: #fde23d;
    color: #fff;
    border-radius: 0 0 10px 0;
    border: none;
    font-size: 30rpx;
    padding: 10rpx 0;
}

.btn-commit.btn_blue {
    background: #74d3ff;
}
.btn-commit.btn_yellow {
    background: #fde23d;
    border-radius: 0 0 0 10px;
}
.btn-commit::after {
    border: none;
}

.weui-label {
    width: 140rpx;
}

.weui-textarea {
    width: 100%;
}

.chose-address {
    padding: 10px 10px;
    font-size: 30rpx;
    color: #fde23d;
}

.chose-address image {
    width: 20px;
    height: 20px;
    margin-right: 5px;
}

.digging_gif {
    position: relative;
    min-height: 200rpx;
}

.digging_gif image {
    width: 100%;
}

.icon-score {
    position: absolute;
    top: 10px;
    left: 10px;
    z-index: 2;
}

.icon-score-pic {
    width: 50rpx;
    vertical-align: middle;
    margin-right: 10rpx;
}

.icon-score .num {
    display: inline-block;
}

.ul-list {
    padding: 17rpx 0 0 25rpx;
}

.btn_digging {
    position: absolute;
    bottom: 30rpx;
    left: 10%;
    width: 80%;
    height: 70rpx;
    line-height: 70rpx;
    border-radius: 70rpx;
    padding: 0;
    font-size: inherit;
}

.btn_light {
    background: #fdf5c5;
}

.btn_yellow {
    background: #fde23d;
}

.btn_gray {
    background: #dcdfe1;
}

.btn_black {
    background: #181713;
}

.progress-bar {
    height: 50rpx;
    line-height: 50rpx;
    margin: 10rpx;
    text-align: center;
    position: relative;
    margin-right: 150rpx;
}

.progress-bar .bgline {
    background: #fde23d;
    position: absolute;
    left: 0;
    top: 0;
    height: 100%;
    border-radius: 70rpx 0 0 70rpx;
}

.progress-bar text {
    position: absolute;
    left: 0;
    top: 0;
    height: 100%;
    width: 100%;
}

.gif-digging {
    position: absolute;
    top: 0;
    left: 0;
}

.btn_speedup {
    position: absolute;
    top: 0;
    right: 0;
    width: 160rpx;
    height: 70rpx;
    line-height: 70rpx;
    border-radius: 70rpx;
    color: #fff;
    font-size: inherit;
}

.winlist-tips {
    margin: 20rpx 50rpx;
    text-align: center;
}

.modalCover{
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    background: rgba(0,0,0,0.8);
    z-index: 100;
    display: flex;
    justify-content: center;
    align-items: center;
}
.modalBody{
    width:70%;
    background: #fff;
    border-radius: 8rpx;
    padding: 40rpx;
    position: relative;
}
.modalBody .imgBox{
    width: 100%;
    height: 600rpx;
}
.modalBody .imgBox image{
    display: block;
    height: 600rpx;
    width: 100%;
}
.modalBody text{
    font-size: 28rpx;
    width: 100%;
    display: block;
    text-align: center;
}
.modalBody .close{
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    bottom: -100rpx;
    left: 50%;
    margin-left: -30rpx;
}
.modalBody .close  image{
    width: 50rpx;
    height: 50rpx;
}