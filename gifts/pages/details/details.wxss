.ul-list {
    padding: 17rpx 0 0 25rpx;
}

.btn_digging {
    position: absolute;
    bottom: 30rpx;
    left: 10%;
    width: 80%;
    height: 70rpx;
    line-height: 70rpx;
    border-radius: 70rpx;
    padding: 0;
    font-size: inherit;
}

.btn_light {
    background: #fdf5c5;
}

.btn_yellow {
    background: #fde23d;
}

.btn_gray {
    background: #dcdfe1;
}

.btn_black {
    background: #181713;
}

.progress-bar {
    height: 50rpx;
    line-height: 50rpx;
    margin: 10rpx;
    text-align: center;
    position: relative;
    margin-right: 150rpx;
}

.progress-bar .bgline {
    background: #fde23d;
    position: absolute;
    left: 0;
    top: 0;
    height: 100%;
    border-radius: 70rpx 0 0 70rpx;
}

.progress-bar text {
    position: absolute;
    left: 0;
    top: 0;
    height: 100%;
    width: 100%;
}

.gif-digging {
    position: absolute;
    top: 0;
    left: 0;
}

.btn_speedup {
    position: absolute;
    top: 0;
    right: 0;
    width: 160rpx;
    height: 70rpx;
    line-height: 70rpx;
    border-radius: 70rpx;
    color: #fff;
    font-size: inherit;
}

.winlist-tips {
    margin: 20rpx 50rpx;
    text-align: center;
}

.modalCover{
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    background: rgba(0,0,0,0.8);
    z-index: 100;
    display: flex;
    justify-content: center;
    align-items: center;
}
.modalBody{
    width:70%;
    background: #fff;
    border-radius: 8rpx;
    padding: 40rpx;
    position: relative;
}
.modalBody .imgBox{
    width: 100%;
    height: 600rpx;
}
.modalBody .imgBox image{
    display: block;
    height: 600rpx;
    width: 100%;
}
.modalBody text{
    padding-top: 30rpx;
    font-size: 28rpx;
    width: 100%;
    display: block;
    text-align: center;
}
.modalBody .close{
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    bottom: -100rpx;
    left: 50%;
    margin-left: -30rpx;
}
.modalBody .close  image{
    width: 50rpx;
    height: 50rpx;
}


/* 道具 */
.coverProperty{
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 100;
    background: rgba(0,0,0,0.8);
}
.dig_treasureBox{
    position: absolute;
    left: 0;
    bottom: -330rpx;
    width: 100%;
    height: 330rpx;
}
.propertyBox{
    width: 600rpx;
    height: 700rpx;
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -300rpx;
    margin-top: -350rpx;
    background: #fff;
    border-radius: 8rpx;
    display: flex;
    flex-direction: column;
}
.propertyBox .close{
    position: absolute;
    width: 50rpx;
    height: 50rpx;
    right: -50rpx;
    top: -50rpx;
}
.propertyBox .close image{
    width: 50rpx;
    height: 50rpx;
}
.propertyBox .propHeader{
    display: flex;
    align-items: center;
    justify-content: space-between;
    height: 100rpx;
    padding: 0 20rpx;
}
.propertyBox .propHeader .btn{
    width: 120rpx;
    height: 60rpx;
    border-radius: 8rpx;
    background: #fde23d;
    text-align: center;
    line-height: 60rpx;
    color: #4a4a4a;
    
}

/* 道具列表 */
.propertyList {
    height: 580rpx;
    overflow-y: auto;
    overflow-x: hidden;
}
.propertyList .item .form{
    display: flex;
    justify-content: space-between;
    flex-direction: row;
    align-items: center;
    padding: 10rpx 20rpx;
    border-bottom: 1px #f5f5f5 solid;
    position: relative;
}
.propertyList .item .form .img{
    flex-basis: 130rpx;
    height: 100rpx;
    position: relative;
}
.propertyList .item .form .img .num{
    position: absolute;
    right: 10rpx;
    bottom: 0rpx;
    font-size: 24rpx;
    background: rgba(255,255,255,0.6);
    padding: 0 5rpx;
}
.propertyList .img image{
    width: 100rpx;
    height: 100rpx;
}
.propertyList .info{
    flex-grow: 1;
}
.propertyList .info .title{
    font-size: 32rpx;
    color: #000;
}
.propertyList .info  .txt{
    color: #acacac;
    font-size: 24rpx;
}
.propertyList .button{
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    opacity: 0;
}

.exchangeBox{
    padding:0 50rpx;
}
.exchangeBox .copy{
    width: 100%;
    height: 80rpx;
    border: 1px #f5f5f5 solid;
    border-radius: 8rpx;
    padding-left: 20rpx;
}
.exchangeBox .copy input{
    margin-top: 10rpx;
    height: 60rpx;
    line-height: 60rpx;
}
.exchangeBox .question{
    padding: 20rpx 0;
    color: #acacac;
}
.exchangeBox .question image{
    width: 40rpx;
    height: 40rpx;
    float: left;
    margin-right: 10rpx;
}
.exchangeBox .btnBox{
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 150rpx;
    padding-bottom: 20rpx;
}
.exchangeBox .btnBox view,.exchangeBox .btnBox button{
    height: 80rpx;
    text-align: center;
    width: 200rpx;
    line-height: 80rpx;
    border-radius: 8rpx;
    box-sizing: border-box;
    font-size: 36rpx;
}

.exchangeBox .btnBox view{
    border:1px #bfbfbf solid;
}
.exchangeBox .btnBox button{
    border:1px #fde23d solid ;
    background: #fde23d;
    color: #4a4a4a;
    padding: 0;
    display: block;
    margin: 0;
}
.getCodeIntro{
    max-height: 600rpx;
    overflow-y: auto;
    overflow-x: hidden;
    padding: 20rpx;
    position: relative;
}
.getCodeIntro text{
    line-height: 60rpx;
}
.getCodeIntroBox{
    position: relative;
}
.getCodeIntroBox .close{
    width:50rpx;
    height: 50rpx;
    position: absolute;
    bottom: -90rpx;
    left: 50%;
    margin-left: -30rpx;
}
.getCodeIntroBox .close image{
    width: 100%;
    height: 100%;
}

