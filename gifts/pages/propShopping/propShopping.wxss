/* pages/propShopping/propShopping.wxss */
page{
    background: #fff;
}
.propertyList .item .form{
    display: flex;
    justify-content: space-between;
    flex-direction: row;
    align-items: center;
    padding: 10rpx 20rpx;
    border-bottom: 1px #f5f5f5 solid;
    position: relative;
}
.propertyList .item .form .img{
    flex-basis: 130rpx;
    height: 100rpx;
    position: relative;
}
.propertyList .img .num{
    position: absolute;
    right: 10rpx;
    bottom: 0rpx;
    font-size: 24rpx;
    background: rgba(255,255,255,0.6);
    padding: 0 5rpx;
}
.propertyList .img image{
    width: 100rpx;
    height: 100rpx;
}
.propertyList .info{
    flex-grow: 1;
}
.propertyList .info .title{
    font-size: 32rpx;
    color: #000;
}
.propertyList .info  .txt{
    color: #acacac;
    font-size: 24rpx;
}
.propertyList .button{
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    opacity: 0;
}
.propertyList .buyBox{
    flex-basis: 140rpx;

}
.propertyList .buyBox .btn{
    margin-left: 20rpx;
    width: 120rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    background: #fde23d;

}