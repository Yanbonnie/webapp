.icon-square:before { content: "\e6d5"; }

.icon-squarecheck:before { content: "\e6d6"; }

.icon-address:before { content: "\e939"; }

.icon-warning-o:before { content: "\e674"; }

.icon-right:before { content: "\e655"; }

.icon-camera:before { content: "\e626"; }


/*color 
    activecolor  0894ec
    graycolor #929292
*/
.mt30{
    margin-top: 30rpx;
}
.formBox{
    padding-left: 40rpx;
    background: #ffffff;
}
.formBox .flexBox{
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 20rpx 0;
    border-bottom: 1px #f5f5f5 solid;
}
.formBox .flexBox .flexL{
    flex-basis: 150rpx;
    font-size: 24rpx;
    display: block;
}
.formBox .flexBox .flexC{
     flex-grow: 1; 
    font-size: 24rpx;
}
.formBox .flexBox .flexC picker{
    width: 100%;
    height: 50rpx;
    line-height: 50rpx;
}
.formBox .flexBox .flexC .gray{
    color: #808080;
}
.formBox .flexBox .flexR{
    flex-basis: 80rpx;
    font-size: 24rpx;
}
.formBox .flexBox .flexR text{
    color: #8a8a8a;
}

.BtnBox{
    padding: 30rpx 40rpx;
}
.BtnBox view{
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    background: #0894ec;
    color: #fff;
    border-radius: 8rpx;
    font-size: 24rpx;
}
.BtnBox .gray{
    background: #ccc;
    color: #f5f5f5;
}




.cityCover{
    position: fixed;
    height: 100%;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    z-index: 10;
    background: #fff;
    border-top-left-radius: 20rpx;
    border-top-right-radius: 20rpx;
}

.operBox{
    display: flex;
    padding: 30rpx 40rpx;
    justify-content: space-between;

}
.operBox view{
    height: 100rpx;
}
.operBox text{
    font-size: 28rpx;   
    color: #808080; 
}
.operBox .iconfont {
    margin-right: 10rpx;
}
.operBox view:nth-child(1) text{
    color: #ff503f;
}
.operBox .icon-squarecheck{
    color: #ff503f;
}
.serverCover{
    position: fixed;
    z-index: 10;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background: rgba(0,0,0,0.8);
}
.serverBox{
    width: 650rpx;
    height: 1000rpx;
    background: #fff;
    border-radius: 10rpx;
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -325rpx;
    margin-top: -500rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    flex-direction: column;
}
.serverBox view{
    font-size: 30rpx;
    text-align: center;
}
.serverBox .title,.serverBox .bottom{
    width: 100%;
    line-height: 80rpx;
    flex-basis: 80rpx;
    display: block;
}
.serverBox .serverContain{
    flex-grow: 1;
    overflow: auto;
    align-self: flex-start;
    text-align: left;
    line-height: 60rpx;
}
.serverBox .serverContain text{
    display: block;
    padding: 0 80rpx;
    word-wrap: break-word;
    text-indent: 60rpx;
}
.serverBox .bottom{
    border-top: 1px #f5f5f5 solid;
}