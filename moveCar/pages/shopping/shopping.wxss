/* pages/shopping/shopping.wxss */
page{
    width: 100%;
    height: 100%;
    background: #f5f5f5;
}
image{
    width: 200rpx;
    height: 200rpx;
}
.shopping{
    width: 100%;
    height: 100%;
}
.shopping .itemBox{
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content:center;
    align-items: center;
    color: #8a8a8a;
    font-size: 30rpx;
}
.shopping .item{
    width: 650rpx;
    height:300rpx;
    line-height: 300rpx;
    margin: 20rpx 0;
    text-align: center;
    background: rgba(0,0,0,0.5);
    position: relative;
    border-radius: 10rpx;
    overflow: hidden;
}
.shopping .item image{
    width: 650rpx;
    height: 300rpx;
}
.shopping .item navigator{
    width: 100%;
    height: 300rpx;
    position: absolute;
    left: 0;
    top: 0;
    line-height: 300rpx;
    font-size: 40rpx;
    color: #000;
}