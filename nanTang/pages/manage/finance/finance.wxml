<!--pages/manage/finance/finance.wxml-->
<view class='finance'>
    <view class='nav'>
        <view class='day'>
            <text>日</text>
            <image class='iconDown' src='/assets/images/down.png'></image>
        </view>
        <view class='dataTime'>
            <text>2018-04-30</text>
            <image class='iconDown' src='/assets/images/down.png'></image>
        </view>
        <view class='income'>收入：1982元</view>
    </view>
    <view class='financeBody'>
        <view class='list'>
            <scroll-view scroll-y="true" style='height:100%;'>
                <view class='item' wx:for="{{[1,2,3,4,5,6,7,8,9]}}" wx:key="{{index}}">
                    <view class='itemL'>
                        <image src='/assets/images/picture.jpg'></image>
                    </view>
                    <view class='itemR'>
                        <view class='txt'>
                            <text>微信名</text>
                            <text>￥48</text>
                        </view>
                        <text class='time'>2018-05-05 22:17:32</text>
                    </view>
                </view>
            </scroll-view>
        </view>
    </view>
</view>